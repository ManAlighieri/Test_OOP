class Animal:
    def sonido(self):
        pass  

class Perro(Animal):
    def sonido(self):
        return "¡Guau!"

class Gato(Animal):
    def sonido(self):
        return "¡Miau!"


def hacer_sonar(animal):
    print(animal.sonido())


mi_perro = Perro()
mi_gato = Gato()
hacer_sonar(mi_perro)  
hacer_sonar(mi_gato)  